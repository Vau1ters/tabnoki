{
  "manifest_version": 2,
  "name": "Tabnoki",
  "version": "0.0.1",

  "description": "All tabs with you",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_title": "タブノキ",
    "default_popup": "popup.html"
  },

  "author": "✨ 💪🌚🍤 ✨",
  "permissions": ["tabs"],
  "version_name": "v0.0.1"
}

